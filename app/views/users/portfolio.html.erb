<h1>Porfolio</h1>
<table>
  <tr>
    <th>Symbol</th>
    <th>Company Name</th>
    <th>Exchange</th>
    <th>Industry</th>
    <th>CEO</th>
    <th>Security Name</th>
    <th>Issue Type</th>
    <th>Sector</th>
    <th>Employees</th>
  </tr>
  <% @stocks.each do |stock| %>
    <% company = @client.company(stock.symbol) %>
    <tr>
      <td>
        <%= link_to company["symbol"], company["website"] %>
      </td>
      <td>
        <%= company["company_name"] %>
      </td>
      <td><%= company["exchange"] %></td>
      <td><%= company["industry"] %></td>
      <td><%= company["ceo"] %></td>
      <td><%= company["security_name"] %></td>
      <td><%= company["issue_type"] %></td>
      <td><%= company["sector"] %></td>
      <td><%= company["employees"] %></td>
    </tr>
  <% end %>
</table>
